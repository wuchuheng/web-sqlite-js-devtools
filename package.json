{
  "name": "web-sqlite-devtools",
  "displayName": "Web Sqlite DevTools",
  "version": "1.0.0",
  "author": "",
  "description": "Chrome DevTools extension for debugging web-sqlite-js databases",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "chrome-extension",
    "react",
    "vite",
    "create-chrome-ext",
    "web-sqlite",
    "sqlite",
    "devtools",
    "database"
  ],
  "engines": {
    "node": ">=14.18.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write '**/*.{tsx,ts,json,css,scss,md}'",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "zip": "npm run build && node src/zip.js"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.8",
    "@types/react-syntax-highlighter": "^15.5.13",
    "clsx": "^2.1.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.3",
    "tailwindcss": "^4.1.8",
    "web-sqlite-js": "^1.0.11"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.26",
    "@eslint/js": "^9.39.2",
    "@types/chrome": "^0.0.246",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-react": "^4.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^17.0.0",
    "gulp": "^5.0.0",
    "gulp-zip": "^6.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^5.4.10"
  }
}
